{
  "private": true,
  "name": "blog",
  "description": "Preact's netlify CMS template",
  "version": "0.1.2",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "build": "preact build --prerenderUrls ./prerender-urls.js",
    "dev": "preact watch --prerenderUrls ./prerender-urls.js",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "@teamsupercell/typings-for-css-modules-loader": "^2.4.0",
    "@types/classnames": "^2.2.11",
    "@types/marked": "^1.2.1",
    "@types/prismjs": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-synacor": "^3.0.5",
    "eslint-plugin-prettier": "^3.3.0",
    "esm": "^3.2.25",
    "fp-ts": "^2.9.1",
    "glob": "^7.1.4",
    "io-ts": "^2.2.13",
    "jest": "^26.6.3",
    "jest-preset-preact": "^4.0.2",
    "markdown": "^0.5.0",
    "md-crawler": "^1.0.4",
    "node-sass": "^5.0.0",
    "node-sass-utils": "^1.1.2",
    "per-env": "^1.0.2",
    "preact-cli": "^3.0.5",
    "preact-render-spy": "^1.2.1",
    "prettier": "^2.2.1",
    "react-syntax-highlighter": "^15.4.3",
    "sass-loader": "^10.1.0",
    "typescript": "^4.1.3",
    "webpack-bundle-analyzer": "^4.3.0"
  },
  "dependencies": {
    "@preact/prerender-data-provider": "^3.0.1",
    "bulma": "^0.9.1",
    "bulma-tooltip": "^3.0.2",
    "classnames": "^2.2.6",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "markdown-to-jsx": "^7.1.0",
    "marked": "^1.2.7",
    "parse-md": "^2.0.4",
    "preact": "^10.5.7",
    "preact-bulma": "^2.0.0-next.3",
    "preact-cli-plugin-netlify": "^1.6.0",
    "preact-markup": "^2.0.0",
    "preact-render-to-string": "^5.1.12",
    "preact-router": "^3.2.1",
    "reading-time": "^1.2.1",
    "ts-node": "^9.1.1"
  },
  "jest": {
    "preset": "jest-preset-preact"
  },
  "prettier": {
    "printWidth": 120
  },
  "bugs": {
    "url": "https://github.com/preactjs/preact-netlify/issues"
  },
  "homepage": "https://github.com/preactjs/preact-netlify#readme",
  "keywords": [
    "preact",
    "netlify"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/preactjs/preact-netlify.git"
