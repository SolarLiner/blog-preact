{
  "private": true,
  "name": "blog",
  "description": "Preact's netlify CMS template",
  "version": "0.1.2",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "build": "preact build --prerenderUrls ./prerender-urls.js",
    "dev": "preact watch --prerenderUrls ./prerender-urls.js",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "@teamsupercell/typings-for-css-modules-loader": "^2.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-synacor": "^3.0.5",
    "eslint-plugin-prettier": "^3.1.2",
    "esm": "^3.2.25",
    "glob": "^7.1.4",
    "jest": "^25.1.0",
    "jest-preset-preact": "^1.0.0",
    "markdown": "^0.5.0",
    "node-sass": "^4.13.1",
    "node-sass-utils": "^1.1.2",
    "per-env": "^1.0.2",
    "preact-cli": "^3.0.0-rc.10",
    "preact-render-spy": "^1.2.1",
    "prettier": "^1.19.1",
    "react-syntax-highlighter": "^12.2.1",
    "sass-loader": "^8.0.2",
    "typescript": "^3.8.3",
    "webpack-bundle-analyzer": "^3.6.1"
  },
  "dependencies": {
    "@preact/prerender-data-provider": "^0.0.1",
    "bulma": "^0.8.0",
    "bulma-tooltip": "^3.0.2",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "markdown-to-jsx": "^6.11.0",
    "parse-md": "^2.0.4",
    "preact": "^10.3.4",
    "preact-bulma": "^2.0.0-next.0",
    "preact-cli-plugin-netlify": "^1.5.0",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1"
  },
  "jest": {
    "preset": "jest-preset-preact"
  },
  "prettier": {
    "printWidth": 120
  },
  "bugs": {
    "url": "https://github.com/preactjs/preact-netlify/issues"
  },
  "homepage": "https://github.com/preactjs/preact-netlify#readme",
  "keywords": [
    "preact",
    "netlify"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/preactjs/preact-netlify.git"
  }
}
